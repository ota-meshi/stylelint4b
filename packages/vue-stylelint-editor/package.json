{
  "name": "vue-stylelint-editor",
  "version": "0.6.0",
  "description": "This package provides Vue.js component of a code editor for stylelint playgrounds.",
  "main": "lib/index.vue",
  "unpkg": "dist/vue-stylelint-editor.js",
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "exit 0",
    "test:chrome": "exit 0",
    "build": "env-cmd -e prod node --max-old-space-size=2048 ./build",
    "build:dev": "env-cmd -e dev node ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/stylelint4b.git"
  },
  "author": "Yosuke Ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/stylelint4b/issues"
  },
  "homepage": "https://ota-meshi.github.io/stylelint4b/vue-stylelint-editor/",
  "devDependencies": {
    "chai": "^5.0.0",
    "css-loader": "^7.0.0",
    "cssnano": "^7.0.0",
    "env-cmd": "^10.1.0",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^10.0.0",
    "url-loader": "^4.1.1",
    "vue": "^3.0.0",
    "vue-loader": "^15.0.0",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "vue-eslint-editor": "^1.1.0"
  }
